<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
      <bean class="com.xworkz.fooditems.FoodItems"  id="foodItems">
      <property name="foodItemsList">
      <list>
      <value>Idli</value>
      <value>Dosa</value>
      <value>curdrice</value>
      </list>
      </property>
      </bean>
      <bean class="com.xworkz.fooditems.Cars"  id="cars">
      <property name="cars">
      <set>
      <value>Swift</value>
      <value>Creta</value>
      <value>Audi</value>
      </set>
      </property>
      </bean>
      <bean class="com.xworkz.fooditems.CapitalCities"  id="capitalCities">
      <property name="capitalCities">
      <map>
      <entry key="karnataka" value="Bangalore"></entry>
      <entry key="Andhra Pradesh" value="Hyderabad"></entry>
      <entry key="Tamil Nadu" value="Chennai"></entry>
      </map>
      </property>
      </bean>
</beans>
